{
  "name": "heroes3tools",
  "version": "2.0.0-alpha.4",
  "private": true,
  "scripts": {
    "start": "vite",
    "build": "cross-env NODE_ENV=production npm run lint && vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "test": "jest",
    "lint": "cross-env NODE_ENV=production eslint --ext .ts,.vue src",
    "lint:fix": "cross-env NODE_ENV=production eslint --ext .ts,.vue --fix src",
    "commit": "npx git-cz"
  },
  "dependencies": {
    "@vueform/multiselect": "^2.3.1",
    "@vueuse/core": "^7.5.3",
    "pinia": "^2.0.9",
    "vue": "^3.2.19",
    "vue-i18n": "^9.2.0-beta.9",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@vitejs/plugin-vue": "^2.0.1",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-prettier-vue": "^3.1.0",
    "eslint-plugin-vue": "^8.2.0",
    "husky": "^7.0.4",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "sass": "^1.46.0",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4",
    "vite": "^2.7.10",
    "vite-plugin-pwa": "^0.11.12",
    "vue-eslint-parser": "^8.0.1",
    "vue-tsc": "^0.30.2",
    "workbox-build": "^6.4.2",
    "workbox-core": "^6.4.2",
    "workbox-precaching": "^6.4.2",
    "workbox-routing": "^6.4.2",
    "workbox-strategies": "^6.4.2",
    "yorkie": "^2.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g ./commitlint.js"
    }
  }
}
